,CAAA,MAAA;AACA,KALA,EAMA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,KARA,CAAA;AAUA,GArCA;AAsCA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA,QAAA,GAAA,KAAA,KAAA;AAAA,UACA,QAAA,GAAA,EADA;AAAA,UAEA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAFA;AAGA,UAAA,QAAA,GAAA,QAAA,GAAA,CAAA,EAAA,IAAA,IAAA,CAAA;AACA,aAAA,IAAA,GAAA,CAAA;AACA;AAPA,GAtCA;AA+CA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAHA;AAIA,IAAA,OAJA,qBAIA;AACA,WAAA,EAAA,GAAA,CAAA;AACA,WAAA,OAAA,CAAA,KAAA,EAAA;AACA,KAPA;AAQA,IAAA,KARA,mBAQA;AACA,WAAA,EAAA;AACA,WAAA,OAAA,CAAA,KAAA,EAAA;AACA,KAXA;AAYA,IAAA,MAZA,oBAYA;AACA,WAAA,EAAA;AACA,WAAA,OAAA,CAAA,KAAA,EAAA;AACA,KAfA;AAgBA,IAAA,KAhBA,mBAgBA;AACA,WAAA,EAAA,GAAA,KAAA,QAAA;AACA,WAAA,OAAA,CAAA,KAAA,EAAA;AACA,KAnBA;AAoBA,IAAA,OApBA,mBAoBA,KApBA,EAoBA;AAAA;;AACA,UAAA,KAAA,GAAA;AACA,QAAA,EAAA,EAAA,KADA;AAEA,QAAA,GAAA,EAAA,EAFA;AAGA,QAAA,GAAA,EAAA,IAHA;AAIA,QAAA,IAAA,EAAA;AAJA,OAAA;AAMA,MAAA,WAAA,CACA,KADA,EAEA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAJA,EAKA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAPA,CAAA;AASA,KApCA;AAqCA,IAAA,QArCA,sBAqCA;AAAA;;AACA,UAAA,KAAA,GAAA;AACA,QAAA,EAAA,EAAA,KAAA,EADA;AAEA,QAAA,GAAA,EAAA,EAFA;AAGA,QAAA,GAAA,EAAA,KAAA,GAHA;AAIA,QAAA,IAAA,EAAA,KAAA;AAJA,OAAA;AAMA,MAAA,WAAA,CACA,KADA,EAEA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAJA,EAKA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAPA,CAAA;AASA;AArDA;AA/CA,CAAA","sourcesContent":["<template>\n  <b-container class=\"bv-example-row mt-3\">\n    <b-row>\n      <b-col>\n        <b-alert show><h3>글목록</h3></b-alert>\n      </b-col>\n    </b-row>\n    <b-row class=\"mb-1\">\n      <b-col class=\"text-right\">\n        <b-button variant=\"outline-primary\" @click=\"moveWrite()\"\n          >글쓰기</b-button\n        >\n      </b-col>\n    </b-row>\n    <b-row>\n      <b-col v-if=\"articles.length\">\n        <b-table-simple hover responsive>\n          <b-thead head-variant=\"dark\">\n            <b-tr>\n              <b-th>글번호</b-th>\n              <b-th>제목</b-th>\n              <b-th>조회수</b-th>\n              <b-th>작성자</b-th>\n              <b-th>작성일</b-th>\n            </b-tr>\n          </b-thead>\n          <tbody>\n            <!-- 하위 component인 ListRow에 데이터 전달(props) -->\n            <board-list-row\n              v-for=\"(article, index) in articles\"\n              :key=\"index\"\n              v-bind=\"article\"\n            />\n          </tbody>\n        </b-table-simple>\n      </b-col>\n      <!-- <b-col v-else class=\"text-center\">도서 목록이 없습니다.</b-col> -->\n    </b-row>\n    <b-row class=\"mb-3\">\n      <b-col class=\"text-right\">\n        <b-button\n          variant=\"outline-primary\"\n          style=\"float: right\"\n          @click=\"searchPg\"\n          >검색</b-button\n        >\n        <b-form-input\n          v-model=\"word\"\n          style=\"width: 250px; float: right\"\n        ></b-form-input>\n        <b-form-select\n          v-model=\"key\"\n          :options=\"options\"\n          style=\"width: 100px\"\n        ></b-form-select>\n      </b-col>\n    </b-row>\n    <b-button-toolbar\n      class=\"mb-5\"\n      key-nav\n      aria-label=\"Toolbar with button groups\"\n      style=\"justify-content: center\"\n    >\n      <b-button-group class=\"mx-1\">\n        <b-button\n          size=\"sm\"\n          variant=\"primary\"\n          :disabled=\"pg == 1\"\n          @click=\"firstPg\"\n          >&laquo;</b-button\n        >\n        <b-button size=\"sm\" variant=\"primary\" :disabled=\"pg == 1\" @click=\"prePg\"\n          >&lsaquo;</b-button\n        >\n      </b-button-group>\n      <span class=\"mt-1\">&nbsp; {{ pg }} / {{ totalCnt }} page&nbsp;</span>\n      <b-button-group class=\"mx-1\">\n        <b-button\n          size=\"sm\"\n          variant=\"primary\"\n          :disabled=\"pg == totalCnt\"\n          @click=\"nextPg\"\n          >&rsaquo;</b-button\n        >\n        <b-button\n          size=\"sm\"\n          variant=\"primary\"\n          :disabled=\"pg == totalCnt\"\n          @click=\"endPg\"\n          >&raquo;</b-button\n        >\n      </b-button-group>\n    </b-button-toolbar>\n  </b-container>\n</template>\n\n<script>\nimport BoardListRow from \"@/components/board/child/BoardListRow\";\nimport { listArticle } from \"@/api/board.js\";\n\nexport default {\n  name: \"BoardList\",\n  components: {\n    BoardListRow,\n  },\n  data() {\n    return {\n      articles: [],\n      total: 0,\n      pg: 1,\n      spp: 15,\n\n      word: null,\n      key: null,\n      options: [\n        { value: \"subject\", text: \"제  목\" },\n        { value: \"userid\", text: \"아이디\" },\n      ],\n    };\n  },\n  created() {\n    let param = {\n      pg: 1,\n      spp: 15,\n      key: null,\n      word: null,\n    };\n    listArticle(\n      param,\n      (response) => {\n        this.articles = response.data;\n        this.total = this.articles.length;\n      },\n      (error) => {\n        console.log(error);\n      }\n    );\n  },\n  computed: {\n    totalCnt() {\n      let listLeng = this.total,\n        listSize = 15,\n        page = Math.floor(listLeng / listSize);\n      if (listLeng % listSize > 0) page += 1;\n      return page + 1;\n    },\n  },\n  methods: {\n    moveWrite() {\n      this.$router.push({ name: \"BoardWrite\" });\n    },\n    firstPg() {\n      this.pg = 1;\n      this.chagePg(this.pg);\n    },\n    prePg() {\n      this.pg--;\n      this.chagePg(this.pg);\n    },\n    nextPg() {\n      this.pg++;\n      this.chagePg(this.pg);\n    },\n    endPg() {\n      this.pg = this.totalCnt;\n      this.chagePg(this.pg);\n    },\n    chagePg(pgNum) {\n      let param = {\n        pg: pgNum,\n        spp: 15,\n        key: null,\n        word: null,\n      };\n      listArticle(\n        param,\n        (response) => {\n          this.articles = response.data;\n        },\n        (error) => {\n          console.log(error);\n        }\n      );\n    },\n    searchPg() {\n      let param = {\n        pg: this.pg,\n        spp: 15,\n        key: this.key,\n        word: this.word,\n      };\n      listArticle(\n        param,\n        (response) => {\n          this.articles = response.data;\n        },\n        (error) => {\n          console.log(error);\n        }\n      );\n    },\n  },\n};\n</script>\n\n<style scope>\n.tdClass {\n  width: 50px;\n  text-align: center;\n}\n.tdSubject {\n  width: 300px;\n  text-align: left;\n}\n.inner {\n  display: inline-block;\n}\n</style>\n"],"sourceRoot":"src/components/board"}]}